{
  "name": "logo-playground",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Online environment for Logo programming language",
  "scripts": {
    "precommit": "lint-staged",
    "build-vendor-dev": "webpack  --config webpack.config.vendor.js",
    "build-app-dev": "webpack  --config webpack.config.js",
    "build-vendor-prod": "webpack  --config webpack.config.vendor.js --env.prod",
    "build-app-prod": "webpack  --config webpack.config.js --env.prod",
    "build": "npm run build-vendor-prod && npm run build-app-prod",
    "test": "npm run build-vendor-dev && karma start karma.conf.js",
    "posttest": "npm run coverage",
    "coverage": "npm run coverage-remap && npm run coverage-report-html && npm run coverage-report-clover",
    "coverage-remap": "remap-istanbul -i reports/coverage/coverage.json -o reports/coverage/coverage.json -t json",
    "coverage-report-html": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage html",
    "coverage-report-clover": "istanbul report --include reports/coverage/coverage.json --dir reports/coverage clover",
    "start": "npm run build-vendor-dev && webpack-dev-server",
    "tslint": "tslint --project . --format verbose",
    "tslint-fix": "tslint --project . --format verbose --fix",
    "translate": "tsc tools/update-translations.ts --lib dom,es7 --target es5 && node tools/update-translations.js"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "clipboard": "2.0.0",
    "codemirror": "5.37.0",
    "color": "3.0.0",
    "core-js": "2.5.5",
    "file-saver": "1.3.8",
    "golden-layout": "1.5.9",
    "inversify": "4.13.0",
    "inversify-inject-decorators": "3.1.0",
    "isomorphic-fetch": "2.2.1",
    "jed": "1.1.1",
    "jquery": "3.3.1",
    "keymaster": "1.6.2",
    "markdown-it": "8.4.1",
    "moment": "2.22.1",
    "po2json": "0.4.5",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.10",
    "spark-md5": "3.0.0",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/chai-as-promised": "7.1.0",
    "@types/classnames": "2.2.3",
    "@types/clipboard": "2.0.0",
    "@types/codemirror": "0.0.56",
    "@types/color": "3.0.0",
    "@types/core-js": "0.9.46",
    "@types/enzyme": "3.1.10",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/esprima": "4.0.1",
    "@types/file-saver": "1.3.0",
    "@types/glob": "5.0.35",
    "@types/history": "4.6.2",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jquery": "3.3.1",
    "@types/keymaster": "1.6.28",
    "@types/markdown-it": "0.0.4",
    "@types/mocha": "5.2.0",
    "@types/node": "9.6.6",
    "@types/react": "16.3.12",
    "@types/react-dom": "16.0.5",
    "@types/react-router": "4.0.23",
    "@types/react-router-dom": "4.2.6",
    "@types/react-test-renderer": "16.0.1",
    "archiver": "2.1.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "esprima": "4.0.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "git-revision-webpack-plugin": "3.0.0",
    "glob": "7.1.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "3.0.1",
    "istanbul-lib-instrument": "1.10.1",
    "json-loader": "0.5.7",
    "karma": "2.0.2",
    "karma-bamboo-reporter": "0.1.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "less": "3.0.2",
    "less-loader": "4.1.0",
    "lint-staged": "7.0.4",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.1.1",
    "pofile": "1.0.10",
    "prettier": "1.12.1",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.3.2",
    "remap-istanbul": "0.11.0",
    "style-loader": "0.21.0",
    "ts-loader": "4.2.0",
    "tslint": "5.9.1",
    "typescript": "2.8.3",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.15",
    "webpack-dev-server": "3.1.3",
    "webpack-notifier": "1.6.0"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx,js,json,css,less}": ["prettier --write", "git add"]
    }
  }
}
